{
  "manifest_version": 3,
  "name": "GitHub1s",
  "version": "0.0.1",
  "description": "GitHub1s Extension",
  "homepage_url": "https://github.com/holazz/webext-github1s",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab"],
  "host_permissions": [
    "https://github.com/*"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "go-to-github1s": {
      "suggested_key": {
        "default": "Ctrl+Right",
        "mac": "Command+Right"
      },
      "description": "Go To GitHub1s"
    }
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [{
    "all_frames": false,
    "matches": [
      "https://github.com/*/*"
    ],
    "js": [
      "content-script.js"
    ],
    "run_at": "document_end"
  }]
}
